<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.nsu.ccfit.muratov.railroad.gui.SelectTableController">
   <children>
      <TextField alignment="TOP_CENTER" editable="false" focusTraversable="false" text="Выбор таблицы для просмотра">
         <font>
            <Font name="DejaVu Sans Bold" size="21.0" />
         </font>
      </TextField>
      <ListView fx:id="listOfTables" prefHeight="206.0" prefWidth="800.0" />
      <Button fx:id="makeChoiceButton" onAction="#onSelectButtonClick" mnemonicParsing="false" text="Выбрать">
         <VBox.margin>
            <Insets top="50.0" />
         </VBox.margin></Button>
      <Button fx:id="backButton" mnemonicParsing="false" onAction="#onBackButtonClick" text="Назад">
         <VBox.margin>
            <Insets top="50.0" />
         </VBox.margin></Button>
   </children>
</VBox>
